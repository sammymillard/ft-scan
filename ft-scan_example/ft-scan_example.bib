@article{tsuda_ketamine_2007,
	title = {Ketamine, an {NMDA}-antagonist, increases the oscillatory frequencies of α-peaks on the electroencephalographic power spectrum},
	volume = {51},
	issn = {00015172 (ISSN)},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-33947209748&doi=10.1111%2Fj.1399-6576.2006.01246.x&partnerID=40&md5=d3d32352511d59578d8d3b62bd27be63},
	doi = {10.1111/j.1399-6576.2006.01246.x},
	abstract = {Background: Ketamine, an N-methyl-D-aspartate (NMDA) antagonist, is known to activate the electroencephalogram (EEG), despite its sedative effects. Spindle oscillations are known to be related to the sedative actions of the reticular thalamic nucleus with links to thalamocortical neurons. This study was designed to examine the effect of ketamine on the spindle oscillations to understand the simultaneous sedative effect and EEG activation that occurs with ketamine, by comparing the EEG in emergence. Methods: Anesthesia was induced with propofol using a target-controlled infusion (TCI) system (3.5 μg/ml). Seventeen patients, scheduled for non-cranial surgery under general anesthesia combined with epidural anesthesia, were randomly divided into two groups: (i) anesthesia was maintained with TCI-propofol alone (n = 8) and (ii) anesthesia was maintained with TCI-propofol and intravenously administered ketamine (n = 9). The EEG was continuously monitored and EEG indices and power spectra were determined. Results: Propofol alone caused the α-peaks of the power spectra to occur at an average frequency of 10.4 ± 0.9 Hz; the addition of ketamine shifted the peaks to higher frequencies of 15.1 ± 1.4 Hz (P {\textbackslash}textless 0.05). On the other hand, when the EEG was activated by discontinuation of propofol, the corresponding α-peaks disappeared. Conclusions: Ketamine increased the frequencies of α-spindle waves induced by propofol, but did not block their formations. The phenomena have the possibility to underlie the cooperative effect between propofol and ketamine concerning sedation and anesthesia. © 2007 Acta Anaesthesiol Scand.},
	language = {English},
	number = {4},
	journal = {Acta Anaesthesiologica Scandinavica},
	author = {Tsuda, N and Hayashi, K and Hagihira, S and Sawa, T},
	year = {2007},
	keywords = {adult, alpha rhythm, electroencephalogram, anesthesia, human, drug activity, male, aged, article, Electroencephalography, drug effect, statistical significance, female, Adult, Female, Male, sedation, power spectrum, major clinical study, Humans, Intravenous, comparative study, Middle Aged, Anesthesia, clinical article, Spectral analysis, priority journal, statistical analysis, oscillatory potential, propofol, Anesthetics, drug potentiation, ketamine, Monitoring, Abdomen, Analgesics, Combined, Elective, endotracheal intubation, Epidural, epidural anesthesia, General, general anesthesia, Intraoperative, Ketamine, lidocaine, muscle relaxation, n methyl dextro aspartic acid receptor blocking ag, N-methyl-D-aspartate, Propofol, Reticular thalamic nucleus, ropivacaine, Spindle wave, Surgical Procedures, Thalamocortical system, vecuronium},
	pages = {472--481},
	annote = {Place: Department of Anesthesiology, Kyoto Prefectural University of Medicine, Kyoto, Japan},
	annote = {TA-Maybe Cited By :24 Export Date: 12 February 2021 CODEN: AANEA Correspondence Address: Hayashi, K.; Department of Anesthesiology, , Kamigyo-ku Kyoto, 602-0841, Japan; email: zukko@koto.kpu-m.ac.jp},
