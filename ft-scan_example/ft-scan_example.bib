}

@article{franzkowiak_cortical_2010,
	title = {Cortical excitability in {Tourette} patients-differential effects of voluntary movements and median nerve stimulation},
	volume = {25},
	url = {http://www3.interscience.wiley.com/cgi-bin/fulltext/123453667/PDFSTART http://ovidsp.ovid.com/ovidweb.cgi?T=JS&PAGE=reference&D=emed11&NEWS=N&AN=70184204},
	doi = {http://dx.doi.org/10.1002/mds.23162},
	abstract = {Objective: To investiagate to what extent cortical excitability is altered in GTS patients by means of event-related desynchronization (ERD) and event-related synchronization (ERS). Background(s): Gilles de la Tourette syndrome (GTS) is a neuropsychiatric disorder characterised by multiple tics. The pathophysiology of GTS is associated with a dysfunction of the basal ganglia possibly yielding cortical excitability changes. Accordingly, in GTS-patients alterations of motor cortex (M1) excitability have been evidenced. Furthermore, sensory processing might be altered in GTS-patients causing increased sensitivity to sensory stimulation. During voluntary movements and sensory stimulation a typical pattern of ERD and ERS occurs. ERD has been associated with an increased excitability of the sensorimotor cortex, whereas ERS may represent cortical deactivation. Method(s): In a first study, 11 GTS-patients and 11 control subjects performed a self-paced finger movement task. In the second study an electrical stimulation was applied to the median nerve of 12 GTS-patients and 12 control subjects. During both tasks neuromagnetic activity was recorded using a 122-channel whole-head magnetoencephalography system. Temporal spectral evolution was calculated for the alpha (8-12 Hz) and beta (13-24 Hz) frequency band, respectively. Result(s): In the movement task, ERD at beta frequency was significantly increased in the contralateral hemisphere in GTS-patients before and during movements. The ERS following movement termination was significantly increased in the ipsilateral M1 and was inversely correlated with tic severity. In contrast, after median nerve stimulation ERD and ERS of the somatosensory cortex in both hemispheres were significantly reduced in GTS-patients at alpha-frequency. Conclusion(s): Our data provide evidence for a pattern of increased activation followed by increased inhibition during voluntary movements. We speculate that the increased activation of contralateral M1 might be related to tic generation whereas increased inhibition of ipsilateral M1 might reflect compensatory activity to suppress tics. The reduced alpha modulation after median nerve stimulation might represent an adaptive mechanism of the somatosensory cortex to inhibit sensory input.},
	language = {English},
	number = {SUPPL. 2},
	journal = {Movement Disorders},
	author = {Franzkowiak, S and Pollok, B and Biermann-Ruben, K and Paszek, J and Thomalla, G and Muenchau, A and Schnitzler, A},
	year = {2010},
	keywords = {sensory stimulation, motor cortex, hemisphere, *patient, somatosensory cortex, pathophysiology, modulation, evoked response, *motor dysfunction, sensorimotor cortex, magnetoencephalography, tic, finger, electrostimulation, *voluntary movement, *Parkinson disease, processing, basal ganglion, *excitability, *median nerve, *nerve stimulation, Gilles de la Tourette syndrome},
	pages = {S527},
	annote = {ISBN: 0885-3185 Publisher: John Wiley and Sons Inc.},
	annote = {TA-Maybe},
	file = {Attachment:files/2930/Franzkowiak-2010-Cortical excitability in Tour.pdf:application/pdf},
}

@article{faber_eeg_2012,
	title = {{EEG} source imaging during two {Qigong} meditations},
	volume = {13},
	url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS&PAGE=reference&D=emed13&NEWS=N&AN=51998836},
	doi = {http://dx.doi.org/10.1007/s10339-012-0441-4},
	abstract = {Experienced Qigong meditators who regularly perform the exercises "Thinking of Nothing" and "Qigong" were studied with multichannel EEG source imaging during their meditations. The intracerebral localization of brain electric activity during the two meditation conditions was compared using sLORETA functional EEG tomography. Differences between conditions were assessed using t statistics (corrected for multiple testing) on the normalized and log-transformed current density values of the sLORETA images. In the EEG alpha-2 frequency, 125 voxels differed significantly; all were more active during "Qigong" than "Thinking of Nothing," forming a single cluster in parietal Brodmann areas 5, 7, 31, and 40, all in the right hemisphere. In the EEG beta-1 frequency, 37 voxels differed significantly; all were more active during "Thinking of Nothing" than "Qigong," forming a single cluster in prefrontal Brodmann areas 6, 8, and 9, all in the left hemisphere. Compared to combined initial-final notask resting, "Qigong" showed activation in posterior areas whereas "Thinking of Nothing" showed activation in anterior areas. The stronger activity of posterior (right) parietal areas during "Qigong" and anterior (left) prefrontal areas during "Thinking of Nothing" may reflect a predominance of self-reference, attention and input-centered processing in the "Qigong" meditation, and of control- centered processing in the "Thinking of Nothing" meditation. © Marta Olivetti Belardinelli and Springer-Verlag 2012.},
	language = {English},
	number = {3},
	journal = {Cognitive Processing},
	author = {Faber, Pascal L and Lehmann, Dietrich and Tei, S and Tsujiuchi, T and Kumano, H and Pascual-Marqui, R D and Kochi, K and Pascal, L and Roberto, D and Faber, Pascal L and Lehmann, Dietrich},
	year = {2012},
	keywords = {*electroencephalogram, adult, consciousness, human, male, article, female, controlled study, *meditation, priority journal, brain region, transcendental meditation, *neuroimaging, image analysis, functional magnetic resonance imaging, low resolution brain electromagnetic tomography, *Qigong meditation, self transcendence, thinking},
	pages = {255--265},
	annote = {From Duplicate 1 (EEG source imaging during two Qigong meditations - Faber, Pascal L; Lehmann, Dietrich; Tei, S; Tsujiuchi, T; Kumano, H; Pascual-Marqui, R D; Kochi, K; Pascal, L; Roberto, D; Faber, Pascal L; Lehmann, Dietrich) From Duplicate 2 (EEG source imaging during two Qigong meditations - Faber, P L; Lehmann, D; Tei, S; Tsujiuchi, T; Kumano, H; Pascual-Marqui, R D; Kochi, K) TA-Maybe From Duplicate 2 (EEG source imaging during two Qigong meditations - Faber, P L; Lehmann, D; Tei, S; Tsujiuchi, T; Kumano, H; Pascual-Marqui, R D; Kochi, K) TA-Maybe},
	annote = {ISBN: 1612-4782 1612-4790 Place: Germany Publisher: Springer Verlag (Tiergartenstrasse 17, Heidelberg D-69121, Germany)},
